  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'rust_web_server'",
      "cargo": {
        "args": [
          "build",
          "--manifest-path=server/Cargo.toml",
          "--bin=rust_web_server",
          "--package=rust_web_server"
        ],
        "filter": {
          "name": "rust_web_server",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}/server"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'rust_web_server'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--manifest-path=server/Cargo.toml",
          "--bin=rust_web_server",
          "--package=rust_web_server"
        ],
        "filter": {
          "name": "rust_web_server",
          "kind": "bin"
        }
      },
      "args": [
        ""
      ],
      "cwd": "${workspaceFolder}/server"
    }
  ]
}
